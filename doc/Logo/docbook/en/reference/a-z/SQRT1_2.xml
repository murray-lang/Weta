<?xml version="1.0" encoding="UTF-8"?>
<article xmlns="http://docbook.org/ns/docbook" version="5.0" xml:lang="en"
        xml:id="SQRT1_2">
    <?dbhtml dir="a-z" ?>
  <sect1>
    <title>SQRT1_2</title>
    <para>
      Return the square root of 1/2.
    </para>
    <sect2>
      <title>Synopsis</title>
      <funcsynopsis>
        <command>SQRT1_2</command>
      </funcsynopsis>
    </sect2>
    <sect2>
      <title>Arguments</title>
        <para>Nil</para>
    </sect2>
    <sect2>
      <title>Returns</title>
    <para>
        Returns the square root of 1/2
    </para>
    </sect2>
    <sect2>
      <title>Example</title>
      <programlisting>
          make "foo SQRT1_2    ; foo == 0.7071067811865476
      </programlisting>
    </sect2>
