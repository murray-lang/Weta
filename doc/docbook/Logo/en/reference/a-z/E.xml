<?xml version="1.0" encoding="UTF-8"?>
<article xmlns="http://docbook.org/ns/docbook" version="5.0" xml:lang="en"
        xml:id="E">
    <?dbhtml dir="a-z" ?>
  <sect1>
    <title>E</title>
    <para>
      Return the constant e (Euler's constant).
    </para>
    <sect2>
      <title>Synopsis</title>
      <funcsynopsis>
        <command>E</command>
      </funcsynopsis>
    </sect2>
    <sect2>
      <title>Arguments</title>
        <para>Nil</para>
    </sect2>
    <sect2>
      <title>Returns</title>
    <para>
        Returns the constant e (Euler's constant).
    </para>
    </sect2>
    <sect2>
      <title>Example</title>
      <programlisting>
          make "foo E           ; foo == 2.718281828459045
          make "foo exp 1       ; foo == 2.718281828459045
          make "foo exp -1      ; foo == 0.36787944117144233
          make "foo exp 0       ; foo == 1
      </programlisting>
    </sect2>
